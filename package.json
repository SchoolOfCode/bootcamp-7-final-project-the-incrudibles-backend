{
  "name": "bootcamp-7-final-project-the-incrudibles-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config ./bin/www",
    "createGraduatesTable": "node -r dotenv/config ./db/scripts/createGraduatesTable.js",
    "dropGraduatesTable": "node -r dotenv/config ./db/scripts/dropGraduatesTable.js",
    "populateGraduatesTable": "node -r dotenv/config ./db/scripts/populateGraduatesTable.js",
    "resetGraduatesTable": "npm run dropGraduatesTable && npm run createGraduatesTable && npm run populateGraduatesTable",
    "createResponsesTable": "node -r dotenv/config ./db/scripts/createResponsesTable.js",
    "dropResponsesTable": "node -r dotenv/config ./db/scripts/dropResponsesTable.js",
    "populateResponsesTable": "node -r dotenv/config ./db/scripts/populateResponsesTable.js",
    "resetResponsesTable": "npm run dropResponsesTable && npm run createResponsesTable && npm run populateResponsesTable"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {}
}
